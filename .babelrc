{
    "presets": [
      "next/babel"
    ],
    "plugins": [
      [
        "styled-components",
        {
          "ssr": true,
          "displayName": true,
          "preprocess": false
        }
      ],
      ["prismjs", {
        "languages": ["javascript", "css", "html", "jsx", "tsx"],
        "plugins": ["line-numbers", "show-language"],
        "css": true
      }]
    ]
  }